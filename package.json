{
  "name": "node-exam",
  "version": "1.0.0",
  "description": "A code test for a position on X-Team.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "ava --watch",
    "test:report": "ava --tap | tap-spec",
    "find": "node dist/index.js"
  },
  "author": "Dylan Bishop",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "6.x.x",
    "babel-runtime": "^6.20.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
